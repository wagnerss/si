<%= semantic_form_for @contact do |f| %>
  <%= f.inputs do %>
    <%= f.input :name %>
    <%= f.input :telephone %>
    <%= f.input :ip %>
    <%= f.input :email %>
    <%= f.input :subject %>
    <%= f.input :message %>
  <% end %>

  <%= f.buttons do %>
    <%= f.commit_button %>
  <% end %>
<% end %>

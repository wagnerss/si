<%= semantic_form_for @contact do |f| %>
  <%= f.inputs do %>
    <%= f.input :name %>
    <%= f.input :telephone %>
    <%= f.input :email %>
    <%= f.input :subject %>
    <%= f.input :message, :as => :text, :input_html => {:rows => 10  } %>
  <% end %>
  <%= f.buttons do %>
    <%= f.commit_button %>
  <% end %>
<% end %>
